{
  "name": "lichess-tournament-maker",
  "version": "2.0.0",
  "description": "Creates Arena and Swiss tournaments for Lichess teams",
  "main": "dist/index.js",
  "license": "MIT",
  "keywords": ["lichess", "chess", "tournament", "arena", "swiss"],
  "devDependencies": {
    "@types/node": "^14.6.0",
    "@types/node-fetch": "^2.5.7",
    "date-fns": "^2.15.0",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch 'src/**/*.ts' --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "create": "ts-node src/index.ts",
    "dry-run": "ts-node -e \"process.env.DRY_RUN='true'; require('./src/index.ts')\"",
    "test": "yarn dry-run"
  }
}
